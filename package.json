{
  "name": "nextjs-with-typescript",
  "version": "0.1.0",
  "private": true,
  "description": "Simple project that shows the usage of Next.js with TypeScript",
  "author": "TOKTOKHAN.DEV<dev@toktokhan.dev>",
  "scripts": {
    "postinstall": "yarn run theme",
    "prepare": "husky install && node_modules/.bin/ls-engines",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:staged": "lint-staged",
    "lint": "tsc --noEmit && eslint .",
    "commit": "tokript commit",
    "clean-up": "node src/scripts/remove-unnecessary.js",
    "tokript": "tokript",
    "theme": "chakra-cli tokens src/styles/theme",
    "theme:open": "code -r node_modules/@chakra-ui/styled-system/dist/theming.types.d.ts",
    "gen:source": "tokript gen:source",
    "gen:api": "tokript gen:api",
    "gen:icon": "tokript gen:icon",
    "gen:img": "tokript gen:img",
    "gen:font": "tokript gen:font",
    "gen:route": "tokript gen:route",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,css,md}\""
  },
  "dependencies": {
    "@chakra-ui/react": "^2.5.1",
    "@chakra-ui/styled-system": "^2.3.3",
    "@chakra-ui/theme-tools": "^2.0.16",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@hookform/resolvers": "^2.8.8",
    "@reduxjs/toolkit": "^1.3.6",
    "@tanstack/react-query": "^4.2.3",
    "@tanstack/react-query-devtools": "^4.2.3",
    "@types/react-syntax-highlighter": "^13.5.2",
    "axios": "^1.3.4",
    "chakra-react-select": "^4.6.0",
    "constate": "^3.3.2",
    "dayjs": "^1.10.6",
    "framer-motion": "^10.5.0",
    "lodash": "^4.17.21",
    "ls-engines": "^0.7.0",
    "next": "^13.2.4",
    "react": "^18.2.0",
    "react-cookie": "^4.1.1",
    "react-datepicker": "^4.10.0",
    "react-device-detect": "^2.1.2",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.5",
    "react-redux": "^7.2.0",
    "react-select": "^5.2.1",
    "react-syntax-highlighter": "^15.5.0",
    "tinycolor2": "^1.4.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.3.0",
    "@tosspayments/payment-sdk": "^1.1.1",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/node": "18.15.11",
    "@types/react": "^18.0.28",
    "@types/react-datepicker": "^4.4.2",
    "@types/react-dom": "^18.0.11",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.36.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": ">=7",
    "lint-staged": "^10.5.4",
    "prettier": "^2.8.4",
    "prettier-plugin-organize-imports": "^3.2.1",
    "tokript": "^0.9.98",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "bash -c tsc --noEmit",
      "yarn eslint . --cache --fix",
      "yarn prettier --write"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  },
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 7.10.0",
    "yarn": ">= 1.22.17"
  },
  "os": [
    "darwin",
    "win64",
    "linux"
  ],
  "engineStrict": true
}
