    "react": "17.0.2",
    "react-cookie": "^4.1.1",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.12.2",
    "react-query": "^3.19.1",
    "react-select": "^5.2.1"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^1.5.0",
    "@testing-library/cypress": "^8.0.1",
