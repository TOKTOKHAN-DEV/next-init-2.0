{
  "name": "nextjs-with-typescript",
  "version": "0.1.0",
  "private": true,
  "description": "Simple project that shows the usage of Next.js with TypeScript",
  "author": "TOKTOKHAN.DEV<dev@toktokhan.dev>",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install",
    "gen:theme-typings": "chakra-cli tokens src/styles/theme",
    "gen:icon": "node ./src/scripts/icon-gen/index.js",
    "gen:api": "node ./src/scripts/api-generator/index.js",
    "open:theme-typings": "code -r node_modules/@chakra-ui/styled-system/dist/declarations/src/theming.types.d.ts",
    "postinstall": "yarn run gen:theme-typings",
    "test": "jest --watch",
    "commit": "cz",
    "format": "format(){ prettier-eslint ${1-\"./**/*.{tsx,ts}\"} --config ./.prettierrc --write; }; format"
  },
  "dependencies": {
    "0auth-sdk": "^0.0.12",
    "@chakra-ui/react": "^1.6.5",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@reduxjs/toolkit": "^1.3.6",
    "axios": "^0.26.0",
    "dayjs": "^1.10.6",
    "framer-motion": "^4",
    "moment": "^2.29.1",
    "next": "^11.0.1",
    "react": "17.0.2",
    "react-cookie": "^4.1.1",
    "react-device-detect": "^2.1.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.20.2",
    "react-query": "^3.19.1",
    "react-redux": "^7.2.0",
    "react-select": "^5.2.1",
    "tinycolor2": "^1.4.2"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^1.5.0",
    "@testing-library/cypress": "^8.0.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.1",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/node": "^17.0.18",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "babel-jest": "^27.2.4",
    "commitizen": "^4.2.4",
    "create-chakra-icons": "^1.1.0",
    "dotenv": "^16.0.0",
    "enquirer": "^2.3.6",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.12.4",
    "faker": "npm:@faker-js/faker@^6.0.0-alpha.5",
    "git-cz": "^4.8.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.4",
    "lint-staged": "^10.5.4",
    "minimist": "^1.2.6",
    "msw": "^0.36.8",
    "ora": "^6.0.1",
    "orval": "^6.5.1",
    "prettier": "^2.2.1",
    "prettier-eslint-cli": "^5.0.1",
    "react-test-renderer": "^17.0.2",
    "swagger-typescript-api": "^9.3.1",
    "ts-import": "^2.0.40",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
